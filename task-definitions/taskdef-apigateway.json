{
  "containerDefinitions": [{
    "name": "api-gateway",
    "image": "449591255785.dkr.ecr.eu-west-1.amazonaws.com/api-gateway",
    "environment": [
      {"name": "WEATHER_SERVICE_URL", "value": "http://3.249.244.148:8080"},
      {"name": "LOCATION_SERVICE_URL", "value": "http://34.245.157.26:8081"},
      {"name": "PORT", "value": "8082"}
    ],
    "portMappings": [{"containerPort": 8082, "protocol": "tcp"}],
    "essential": true,
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "/ecs/api-gateway",
        "awslogs-region": "eu-west-1",
        "awslogs-stream-prefix": "ecs"
      }
    }
  }],
  "requiresCompatibilities": ["FARGATE"],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::449591255785:role/ecsTaskExecutionRole",
  "family": "api-gateway"
}